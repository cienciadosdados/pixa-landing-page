      "motion",
      "design",
      "websites"
    ],
    "author": "Paul",
    "license": "MIT",
    "devDependencies": {
      "tailwindcss": "^3.4.1"
    },
    "dependencies": {
      "postcss": "^8.4.33",
      "postcss-simple-vars": "^7.0.1"
    }
  }
  